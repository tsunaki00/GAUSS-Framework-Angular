{
  "name": "gauss-backend",
  "version": "1.0.0",
  "description": "gauss backend",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "seed": "ts-node -r tsconfig-paths/register src/seed.ts",
    "seed:prod": "export NODE_ENV=staging;ts-node -r tsconfig-paths/register src/seed.ts",
    "develop": "export NODE_ENV=develop;nest start --watch",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "export NODE_ENV=production;node dist/seed;node dist/main"
  },
  "dependencies": {
    "@nest-middlewares/express-session": "^6.0.1",
    "@nest-modules/mailer": "^1.1.3",
    "@nestjs/common": "^6.7.2",
